{
  "name": "@mustafa-alhasanat/raven-nest",
  "version": "0.0.392development",
  "description": "Build Nest.js common blocks and files insanely faster!",
  "author": "Mustafa Alhasanat",
  "license": "ISC",
  "main": "./bin/index.js",
  "exports": "./bin/index.js",
  "type": "module",
  "bin": {
    "ravennest": "bin/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "commit": "git-cz",
    "start": "node bin/index.js",
    "build": "rimraf ./bin && tsc",
    "install-g": "npm install -g .",
    "globalize": "npm run build && npm run install-g",
    "acp": "git add . && npm run commit && git push",
    "semantic-release": "semantic-release",
    "raven": "node ./bin/index.js",
    "dev": "npm run globalize && npm run raven",
    "format": "prettier --write .",
    "prerelease": "npm run build",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MustafaHasanat/raven-nest.git"
  },
  "keywords": [
    "nest",
    "backend",
    "blocks",
    "api",
    "cli-tool",
    "npm-tool"
  ],
  "bugs": {
    "url": "https://github.com/MustafaHasanat/raven-nest/issues"
  },
  "homepage": "https://github.com/MustafaHasanat/raven-nest#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "files": [
    "index.js",
    "lib",
    "lib/**/*.(ts|js|txt)",
    "bin/lib/",
    "templates",
    "templates/**/*.(ts|js|txt)",
    "bin/templates/",
    "bin/lib/templates/"
  ],
  "dependencies": {
    "commander": "^11.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "express": "^4.18.2",
    "figlet": "^1.7.0",
    "fs": "^0.0.1-security",
    "inquirer": "^9.2.11",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/figlet": "^1.5.7",
    "@types/htmlparser2": "^3.10.6",
    "@types/inquirer": "^9.0.6",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "semantic-release": "^23.0.0"
  }
}